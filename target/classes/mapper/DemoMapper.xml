<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.demo.mapper.DemoMapper">

    <select id="selectAllCoffee" resultType="com.demo.entity.Coffee">
        select  * from coffee
    </select>

    <select id="selectOneCoffee" resultType="com.demo.entity.Coffee">
        select * from coffee where coffeeId=#{coffeeId}
    </select>

</mapper>

